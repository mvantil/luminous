<template context="field">
    <div>
        <h2 data-bind="text: {{field}}"/>
    </div>
</template>
<template context="field" writable="yes">
    <div>
        <h2 data-bind="text: {{field}}"/>
    </div>
</template>
<template context="listField">
    <span>
        <h2 data-bind="text: {{field}}"/>
    </span>
</template>
<template context="listField" writable="yes">
    <span>
        <h2 data-bind="text: {{field}}"/>
    </span>
</template>
